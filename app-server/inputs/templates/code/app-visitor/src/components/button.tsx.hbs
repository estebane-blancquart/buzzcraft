/*
 * FAIT QUOI : Composant Button généré automatiquement
 * REÇOIT : text: string, href: string, styleOptions: object
 * RETOURNE : JSX Element
 */

interface ButtonProps {
  text: string;
  href: string;
  className?: string;
}

export default function Button({ text, href, className = "" }: ButtonProps) {
  return (
    <a 
      href={href}
      className={`bg-{{styleOptions.bg}} text-{{styleOptions.color}} px-4 py-2 rounded inline-block ${className}`}
    >
      {{content.text}}
    </a>
  );
}